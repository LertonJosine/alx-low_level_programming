#include <stdio.h>

/**
 *main - tells the multiples of 3 or 5 or both
 */
int main(void)
{

int c;

for ( c = 1; c <= 100; c++)
{
if ((c % 3) == 0)
{
printf("Fizz");
}
if ((c % 5) == 0)
{
printf("Buzz");
}
 if (c % 3 != 0 && c % 5 != 0)
{
printf("%d", c);
{
if (c % 3 == 0 && c % 5 == 0)
{
printf("FizzBuzz");
}
}
}
if (c < 100)
{
putchar(' ');
}
}
}
